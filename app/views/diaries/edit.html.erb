<%= form_with(model: @diary, local: true) do |f| %>

  <div class="title">
    <%= f.label :feeling %>
    <%= f.text_field :feeling %>
  </div>

    <div class="time">
    <%= f.label :start_time %>
    <%= f.date_select :start_time %>
  </div>

  <div class="content">
    <%= f.label :body %>
    <%= f.text_field :body %>
  </div>


  <div class="submit">
    <%= f.submit %>
  </div>

<% end %>